{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/components": [
        "./app/components/*"
      ],
      "@/constants": [
        "./app/constants/*"
      ],
      "@icons": [
        "./assets/icons/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "declarations.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
